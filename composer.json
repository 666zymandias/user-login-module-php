{
  "name": "deg540/php-testing-boilerplate",
  "description": "Php testing boilerplate",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "UserLoginService\\": "src/",
      "UserLoginService\\Test\\": "tests/Application"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UserLoginService\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "test": [
      "phpunit --configuration phpunit.xml"
    ]
  }
}
